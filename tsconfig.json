{
  "compilerOptions": {
    "paths": {
      "@/components/*": [
        "components/*"
      ],
      "@/templates/*": [
        "templates/*"
      ],
      "@/assets/icons": [
        "assets/icons/index.tsx"
      ],
      "@/assets/img/*": [
        "assets/img/*"
      ],
      "@/static/*": [
        "static/*"
      ]
    },
    "target": "es2016",
    "module": "esnext",
    "baseUrl": ".",
    "allowJs": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": true,
    "skipLibCheck": true,
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "noEmit": true,
    "incremental": true,
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve"
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx",
    "assets/icons/index.tsx",
    "components/layouts/MainTemplate.jsx",
    "tailwind.config.js"
  ],
  "exclude": [
    "node_modules"
  ]
}
